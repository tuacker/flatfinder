{
  "name": "flatfinder",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "scrape:wohnberatung:login": "tsx src/scrapers/wohnberatung/login.ts",
    "build:ui": "tsc -p ui/tsconfig.json && cp ui/styles.css public/app.css",
