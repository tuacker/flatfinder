  "dependencies": {
    "cheerio": "^1.0.0",
    "express": "^4.21.2",
    "playwright": "^1.51.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.16.5",
    "oxfmt": "0.24.0",
    "oxlint": "^1.39.0",
    "tsx": "^4.20.0",
    "typescript": "^5.7.3"
  }
}
